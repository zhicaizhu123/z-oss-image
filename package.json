{
  "name": "z-oss-image",
  "version": "0.1.0",
  "description": "a plugin of tool to handle oss image",
  "main": "lib/index.js",
  "module": "lib/index.umd.js",
  "typings": "lib/types/index.d.ts",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts}'",
    "dev": "rollup -w -c --environment FORMAT:es",
    "build:es": "rollup -c --environment FORMAT:es",
    "build:umd": "rollup -c --environment FORMAT:umd",
    "build:min": "rollup -c --environment FORMAT:min",
    "build": "rimraf lib/* && run-p build:es build:umd build:min"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhicaizhu123/zOssImage.git"
  },
  "keywords": [
    "oss-image",
    "image-quality",
    "image-resize",
    "image-blur",
    "image-crop",
    "image-watermask"
  ],
  "author": {
    "name": "Peter Chu",
    "email": "zhicaizhu123@126.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^7.18.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "lodash.merge": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.37.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,ts,json}": [
      "prettier --write"
    ]
  }
}
